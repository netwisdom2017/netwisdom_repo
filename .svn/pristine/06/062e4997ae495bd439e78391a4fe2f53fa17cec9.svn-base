package com.dev.warehouse.response;

/**
 * 返回APP
 * @author 81909
 *
 */
public class ResponseBean {
	// http 状态码
    private int code;
 
    // 返回信息
    private String msg;
 
    // 返回的数据
    private Object data;
    
    // 返回的辅助数据
    private Object subData;
 
    public ResponseBean(int code, String msg, Object data) {
        this.code = code;
        this.msg = msg;
        this.data = data;
    }
    
    public ResponseBean(int code, String msg, Object data, Object subData) {
        this.code = code;
        this.msg = msg;
        this.data = data;
        this.subData = subData;
    }
 
    public int getCode() {
        return code;
    }
 
    public void setCode(int code) {
        this.code = code;
    }
 
    public String getMsg() {
        return msg;
    }
 
    public void setMsg(String msg) {
        this.msg = msg;
    }

	public Object getData() {
		return data;
	}

	public void setData(Object data) {
		this.data = data;
	}

	public Object getSubData() {
		return subData;
	}

	public void setSubData(Object subData) {
		this.subData = subData;
	}

}
