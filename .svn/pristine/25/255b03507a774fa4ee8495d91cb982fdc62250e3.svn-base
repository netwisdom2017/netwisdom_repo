<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dev.warehouse.bus.mapper.ExerciseMapper">
    <!--選択された学習進捗情報に関わる「問題情報」を削除する-->
    <delete id="delExercisesByStudyScheduleId">
        delete from exercise where study_schedule_id = #{studyScheduleId}
    </delete>
</mapper>
